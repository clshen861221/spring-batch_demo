--mysql
DROP TABLE IF EXISTS person;
CREATE TABLE person  (
    name VARCHAR(200),
    age VARCHAR(200),
    nation VARCHAR(200),
    address VARCHAR(200)
)ENGINE=MyISAM DEFAULT CHARSET=utf8;

